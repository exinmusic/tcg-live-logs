{
  "name": "tcg-live-logs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "download:common": "node scripts/download-common-cards.js",
    "download:set": "node scripts/download-card-images.js --set",
    "download:cards": "node scripts/download-card-images.js --cards",
    "download:log": "node scripts/download-from-log.js",
    "download:recent": "node scripts/download-card-images.js --sets me2pt5,me2,me1,zsv10pt5,rsv10pt5,sv10,sv9,sv8pt5,sv8",
    "download:limitless": "node scripts/download-from-limitless.js --set",
    "list:sets": "node scripts/list-recent-sets.js",
    "rebuild:manifest": "node scripts/rebuild-manifest.js"
  },
  "dependencies": {
    "cheerio": "^1.2.0",
    "pixelarticons": "^2.0.2",
    "pokemon-tcg-sdk-typescript": "^1.3.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv": "^17.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fast-check": "^4.5.3",
    "globals": "^16.5.0",
    "jsdom": "^28.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^8.0.0-beta.13",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "vite": "^8.0.0-beta.13"
  }
}
